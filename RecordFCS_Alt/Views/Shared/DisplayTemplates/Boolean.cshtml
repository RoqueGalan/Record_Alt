@model bool?

@if (Model.HasValue)
{
    if (Model.Value)
    {
        <span class="text-black"><i class="fa fa-check"></i></span>

    }
    else
    {
        <span class="text-muted"><i class="fa fa-times"></i></span>

    }
}
        @*else
            {
                <span class="badge badge-default">Sin definir</span>
            }*@





